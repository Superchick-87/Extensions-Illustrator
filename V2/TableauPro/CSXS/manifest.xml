<?xml version="1.0" encoding="UTF-8"?>
<ExtensionManifest Version="7.0" ExtensionBundleId="com.NicoSoftware.TableauPro" ExtensionBundleVersion="1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <ExtensionList>
        <Extension Id="com.NicoSoftware.TableauPro.extension" Version="1.0" />
    </ExtensionList>
    <ExecutionEnvironment>
        <HostList>
            <Host Name="ILST" Version="[18.0,99.9]" />
        </HostList>
        <LocaleList>
            <Locale Code="All" />
        </LocaleList>
        <RequiredRuntimeList>
            <RequiredRuntime Name="CSXS" Version="9.0" />
        </RequiredRuntimeList>
    </ExecutionEnvironment>
    <DispatchInfoList>
        <Extension Id="com.NicoSoftware.TableauPro.extension">
            <DispatchInfo >
                <Resources>
                    <MainPath>./client/index.html</MainPath>
                    <ScriptPath>./host/index.jsx</ScriptPath>
                    <CEFCommandLine>
                        <Parameter>--enable-nodejs</Parameter>
                        <Parameter>--remote-debugging-port=8088</Parameter>
                    </CEFCommandLine>
                </Resources>
                <Lifecycle>
                    <AutoVisible>true</AutoVisible>
                </Lifecycle>
                <UI>
                    <Type>Panel</Type>
                    <Menu>Tableau Pro CSV</Menu>
                    <Geometry>
                        <Size>
                            <Height>600</Height>
                            <Width>500</Width>
                        </Size>
                        <MinSize>
                            <Height>400</Height>
                            <Width>400</Width>
                        </MinSize>
                        <MaxSize>
                            <Height>1200</Height>
                            <Width>800</Width>
                        </MaxSize>
                    </Geometry>
                    <Icons>
                      <!--   <Icon Type="Normal">./icons/Light.png</Icon>
                        <Icon Type="RollOver">./icons/Light.png</Icon>
                        
                        <Icon Type="DarkNormal">./icons/Dark.png</Icon>
                        <Icon Type="DarkRollOver">./icons/Dark.png</Icon> -->
                          <Icon Type="DarkNormal">./icons/Light.png</Icon>
                        <Icon Type="DarkRollOver">./icons/Light.png</Icon>
                        
                        <Icon Type="Normal">./icons/Dark.png</Icon>
                        <Icon Type="RollOver">./icons/Dark.png</Icon>
                    </Icons>
                </UI>
            </DispatchInfo>
        </Extension>
    </DispatchInfoList>
</ExtensionManifest>